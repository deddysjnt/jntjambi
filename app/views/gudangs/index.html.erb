<p id="notice"><%= notice %></p>

<h1>Gudangs</h1>

<table>
  <thead>
    <tr>
      <th>Droppoint</th>
      <th>Nama gudang</th>
      <th>Area layanan</th>
      <th>Kota</th>
      <th>Kecamatan</th>
      <th>Alamat</th>
      <th>No telp</th>
      <th>Mobile phone</th>
      <th>Contact</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gudangs.each do |gudang| %>
      <tr>
        <td><%= gudang.droppoint %></td>
        <td><%= gudang.nama_gudang %></td>
        <td><%= gudang.area_layanan %></td>
        <td><%= gudang.kota %></td>
        <td><%= gudang.kecamatan %></td>
        <td><%= gudang.alamat %></td>
        <td><%= gudang.no_telp %></td>
        <td><%= gudang.mobile_phone %></td>
        <td><%= gudang.contact %></td>
        <td><%= gudang.email %></td>
        <td><%= link_to 'Show', gudang %></td>
        <td><%= link_to 'Edit', edit_gudang_path(gudang) %></td>
        <td><%= link_to 'Destroy', gudang, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gudang', new_gudang_path %>
