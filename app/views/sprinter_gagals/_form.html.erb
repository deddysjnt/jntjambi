
<%= simple_form_for(@sprinter_gagal) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :sprinter_kembali_id %>
    <%= f.input :tanggal %>
    <%= f.input :karyawan_id %>
    <%= f.input :droppoint_id %>
    <%= f.input :awb %>
    <%= f.input :masalah %>
    <%= f.input :paket %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
